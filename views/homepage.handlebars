{{#each posts as |post| }}
<div class="card">
    <div class="header">
        <h1>{{post.title}}</h1>
        <p>{{format_date post.date_created}}</p>
        <p>{{user.username}}
    </div>

        <div class="main">
            <p>{{post.content}}</p>
        </div>
    <a href="/post/{{this.id}}"><button>Go to post</button></a>
</div>
{{/each}}